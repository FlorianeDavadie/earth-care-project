
<br>
<br>
<div class="container calendar-title text-center"><h1> <%= current_user.first_name %>'s Calendar</h1></div>


<div class="calendar-container">
  <%= month_calendar(events: @missions, attribute: :start_date, end_attribute: :end_date) do |date, missions| %>
    <%= date %>


      <% missions.each do |mission| %>
        <div class="meeting">
          <%= mission.title %>
        </div>
      <% end %>
  <% end %>
</div>
